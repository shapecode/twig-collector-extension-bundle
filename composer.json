{
    "name": "shapecode/twig-collector-extension-bundle",
    "description": "Symfony Bundle for shapecode/twig-collector-extension.",
    "keywords": [
        "shapecode",
        "symfony",
        "bundle",
        "twig",
        "extension",
        "collect",
        "collector",
        "capture"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "http://shapecode.de",
    "support": {
        "email": "support@nikita-loges.de",
        "issues": "https://bitbucket.org/shapecode/symfony-twig-collector-extension-bundle/issues",
        "source": "https://bitbucket.org/shapecode/symfony-twig-collector-extension-bundle/downloads",
        "wiki": "https://bitbucket.org/shapecode/symfony-twig-collector-extension-bundle/wiki/Home"
    },
    "authors": [
        {
            "name": "Nikita Loges",
            "homepage": "http://nikita-loges.de",
            "email": "develop@nikita-loges.de"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",

        "symfony/dependency-injection": "~2.8|~3.0",
        "symfony/http-kernel": "~2.8|~3.0",
        "symfony/config": "~2.8|~3.0",
        "symfony/twig-bundle": "~2.8|~3.0",

        "shapecode/twig-collector-extension": "~1.1"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "~2.8|~3.0"
    },
    "autoload": {
        "psr-4": {
            "Shapecode\\Bundle\\TwigCollectorBundle\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Shapecode\\Bundle\\TwigCollectorBundle\\Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-release/1.1": "1.1-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
