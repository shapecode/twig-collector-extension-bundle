{
    "name": "shapecode/twig-collector-extension-bundle",
    "description": "Symfony Bundle for shapecode/twig-collector-extension.",
    "keywords": [
        "shapecode",
        "symfony",
        "bundle",
        "twig",
        "extension",
        "collect",
        "collector",
        "capture"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "homepage": "https://github.com/shapecode/twig-collector-extension-bundle",
    "support": {
        "email": "support@nikita-loges.de",
        "issues": "https://github.com/shapecode/twig-collector-extension-bundle/issues",
        "source": "https://github.com/shapecode/twig-collector-extension-bundle/releases",
        "wiki": "https://github.com/shapecode/twig-collector-extension-bundle/wiki"
    },
    "authors": [
        {
            "name": "Nikita Loges",
            "homepage": "http://nikita-loges.de",
            "email": "develop@nikita-loges.de"
        }
    ],
    "require": {
        "php": "~5.5|~7.0",

        "symfony/dependency-injection": "~2.8|~3.0|~4.0",
        "symfony/http-kernel": "~2.8|~3.0|~4.0",
        "symfony/config": "~2.8|~3.0|~4.0",
        "symfony/twig-bundle": "~2.8|~3.0|~4.0",

        "shapecode/twig-collector-extension": "~1.3"
    },
    "autoload": {
        "psr-4": {
            "Shapecode\\Bundle\\TwigCollectorBundle\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
